{
    "name": "symfony-cmf/block-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CMF Block Bundle",
    "keywords": [
        "Symfony CMF",
        "Block",
        "content fragments"
    ],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/BlockBundle/contributors"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "^5.0 || ^6.0 || ^7.0",
        "doctrine/phpcr-bundle": "^3.0",
        "doctrine/phpcr-odm": "^2.0",
        "sonata-project/block-bundle": "^3.11 || ^4.0"
    },
    "require-dev": {
        "symfony-cmf/testing": "^3.0",
        "symfony-cmf/menu-bundle": "dev-master",
        "symfony/phpunit-bridge": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "sonata-project/cache-bundle": "^3.3",
        "twig/twig": "^1.14.2 || ^2.0",
        "symfony/security-bundle": "^5.0 || ^6.0 || ^7.0",
        "symfony/twig-bundle": "^5.0 || ^6.0 || ^7.0",
        "symfony/asset": "^5.0 || ^6.0 || ^7.0",
        "symfony/templating": "^5.0 || ^6.0 || ^7.0",
        "jackalope/jackalope": "^1.4",
        "jackalope/jackalope-doctrine-dbal": "^1.7"
    },
    "suggest": {
        "sonata-project/cache-bundle": "To add caching support for block loading",
        "eko/feedbundle": "When using the RssBlock"
    },
    "repositories": [
        { "type": "vcs", "url": "git@github.com:NETFORMIC-GmbH/symfony-cmf-menu-bundle.git" }
    ],
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\BlockBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\BlockBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "conflict": {
        "sonata/cache": "<1.1.1"
    }
}
